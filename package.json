{
  "name": "vscode-tiddlyedit",
  "repository": {
    "type": "git",
    "url": "https://github.com/byzheng/vscode-tiddlyedit.git"
  },
  "displayName": "TiddlyEdit",
  "publisher": "byzheng",
  "license": "MIT",
  "description": "Seamlessly edit and manage TiddlyWiki tiddlers within VS Code using a REST API connection.",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.102.0"
  },
  "extensionDependencies": [
    "joshua-fontany.tw5-syntax"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "TiddlyEdit Configuration",
      "properties": {
        "tiddlywiki.host": {
          "type": "string",
          "default": "http://127.0.0.1:8080",
          "description": "Base URL of the TiddlyWiki server"
        },
        "tiddlywiki.recipe": {
          "type": "string",
          "default": "default",
          "description": "Recipe name"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tiddlywiki-container",
          "title": "TiddlyWiki",
          "icon": "media/Tiddlywiki.svg"
        }
      ]
    },
    "views": {
      "tiddlywiki-container": [
        {
          "id": "tiddlywiki-tiddlers",
          "name": "Tiddlers",
          "type": "webview"
        },
        {
          "id": "tiddlywiki-meta",
          "name": "Meta",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "tiddlyedit.insertAutocomplete",
        "title": "TiddlyWiki: Insert Autocomplete"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "tiddlyedit.insertAutocomplete",
          "when": "editorTextFocus"
        }
      ]
    },
    "keybindings": [
      {
        "key": "ctrl+space",
        "command": "tiddlyedit.insertAutocomplete",
        "when": "editorTextFocus"
      }
    ]
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  }
}